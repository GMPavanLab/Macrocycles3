WHOLEMOLECULES ENTITY0=1-100

gyr: GYRATION ATOMS=1-100

# C-beta (H for Glycine)
d1: DISTANCE ATOMS=14,54
# Charged Ns 
d2: DISTANCE ATOMS=88,37


METAD ...
  LABEL=metad
  FILE=HILLS
  ARG=d1,d2
  GRID_MIN=0.,0.
  GRID_MAX=2.8,1.5
  SIGMA=0.05,0.05
  PACE=500
  CALC_RCT
  TEMP=298.
  HEIGHT=1.2
  BIASFACTOR=25
... METAD



PRINT ARG=d1,d2,metad.* FILE=metad_data.dat STRIDE=500
PRINT ARG=d1,d2,gyr FILE=distances.dat STRIDE=500

FLUSH STRIDE=500
